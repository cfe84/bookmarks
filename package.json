{
  "name": "bookmarks",
  "version": "1.0.0",
  "description": "Bookmark server",
  "private": true,
  "main": "./dist/App.js",
  "scripts": {
    "start": "node ./dist/server.js || exit 1",
    "clean": "rm -r ./dist ; rm -r testdata",
    "watch:be": "nodemon --watch ./src/backend/ -e 'js,ts' --exec npm run build-n-run:be",
    "build-n-run:be": "tsc && npm run start",
    "build:be": "tsc || exit 1",
    "build:fe": "webpack --mode development || exit 1",
    "watch:fe": "nodemon --watch ./src/frontend -e 'vue,js,css,html' --exec ./node_modules/.bin/webpack -- --mode development || exit 1",
    "build:fe-prod": "webpack --mode production",
    "build": "npm run build:frontend && npm run build:backend",
    "test": "mocha --compilers ts:ts-node/register test/**/*.ts || exit 1",
    "watch:test": "nodemon --watch ./src/backend --watch ./test -e 'js,ts' --exec npm run test",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.org/cfe84/bookmarks"
  },
  "keywords": [
    "bookmark"
  ],
  "author": "cfe84",
  "license": "ISC",
  "dependencies": {
    "atob": "^2.1.1",
    "azure-storage": "^2.9.0-preview",
    "body-parser": "^1.18.3",
    "btoa": "^1.2.1",
    "express": "^4.16.3",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "uuid": "^3.2.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/azure": "^0.9.19",
    "@types/express": "^4.16.0",
    "@types/node": "^10.3.2",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.14",
    "moqjs": "^1.1.2",
    "nodemon": "^1.17.5",
    "should": "^13.2.1",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.1",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8"
  }
}
