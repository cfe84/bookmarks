{
  "name": "bookmarks",
  "version": "1.0.0",
  "description": "Bookmark server",
  "private": true,
  "main": "./dist/App.js",
  "watch": {
    "build:be": "./src/backend/**/*",
    "build:fe": "./src/frontend/**/*"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "clean": "rm -r ./lib/* ; rm -r testdata",
    "watch:be": "nodemon --watch ./src/backend --exec ./node_modules/.bin/ts-node -- ./src/backend/server.ts",
    "build:be": "tsc",
    "build:fe": "webpack --mode development",
    "watch:fe": "nodemon --watch ./src/frontend --exec ./node_modules/.bin/webpack -- --mode development",
    "build:fe-prod": "webpack --mode production",
    "build": "npm run build:frontend && npm run build:backend",
    "test": "mocha --compilers ts:ts-node/register test/**/*.ts",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.org/cfe84/bookmarks"
  },
  "keywords": [
    "bookmark"
  ],
  "author": "cfe84",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.3.2",
    "azure-storage": "^2.9.0-preview",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "nttjs": "^1.4.0",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "ts-node": "^6.1.0",
    "uuid": "^3.2.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/azure": "^0.9.19",
    "copy-webpack-plugin": "^4.5.1",
    "file-loader": "^1.1.11",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.14",
    "nodemon": "^1.17.5",
    "npm-watch": "^0.3.0",
    "should": "^13.2.1",
    "typescript": "^2.9.1",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8"
  }
}
